{
  "name": "@northscaler/service-support",
  "version": "0.2.0-rc.3",
  "description": "Artifacts that assist in writing service layers effectively",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/northscaler-public/service-support.git"
  },
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm install && npm test",
    "unit-integration": "nyc --check-coverage -x 'src/test' --exclude-after-remap false mocha 'src/test/unit/**/*.spec.js' 'src/test/integration/**/*.spec.js'",
    "integration": "nyc --check-coverage -x 'src/test' --exclude-after-remap false mocha 'src/test/integration/**/*.spec.js'",
    "i": "mocha 'src/test/integration/**/*.spec.js'",
    "unit": "nyc --check-coverage -x 'src/test' --exclude-after-remap false mocha 'src/test/unit/**/*.spec.js'",
    "u": "mocha 'src/test/unit/**/*.spec.js'",
    "test": "run-s unit-integration",
    "posttest": "run-s lint",
    "report": "nyc report --reporter=html",
    "cov": "open coverage/index.html",
    "lint": "standard --verbose 'src/**/*.js'",
    "format": "standard --fix 'src/**/*.js'",
    "make-distribution": "run-p doc lic",
    "lic": "node lic.js > LICENSE",
    "doc": "jsdoc --verbose -d docs -r -c jsdoc.json -R README.md src/main"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@northscaler/aspectify": "1.1.1",
    "@northscaler/enum-support": "4.0.4",
    "@northscaler/error-support": "3.0.2",
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-proposal-throw-expressions": "7.8.3",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "acorn": "7.1.1",
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "copyfiles": "2.2.0",
    "dirty-chai": "2.0.1",
    "fx": "18.0.1",
    "intercept-stdout": "0.1.2",
    "jsdoc": "3.6.4",
    "lodash.template": "4.5.0",
    "mocha": "7.1.2",
    "npm-cli-login": "0.1.1",
    "nyc": "15.0.1",
    "semver-sort": "0.0.4",
    "standard": "14.3.3"
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-optional-chaining",
        {
          "loose": false
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-throw-expressions"
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "mocha": {
    "require": "@babel/register"
  }
}
